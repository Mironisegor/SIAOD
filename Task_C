//Структура map

#include <iostream>
//#include <set>
//#include <cstring>
#include <string>
#include <map>

using namespace std;

int main() {
	int n;
	cin >> n;
	map <string, int> set;
	for (int i = 0; i < n; i++) {
		int oper;
		cin >> oper;
		if (oper == 1) {
			string name;
			int money;
			cin >> name >> money;
			set[name] += money;
		}
		else {
			string name;
			cin >> name;
			if (!set.count(name)) {
				cout << "ERROR" << endl;
			}
			else {
				cout << set[name] << endl;
			}
		}
	}
	return 0;
}
