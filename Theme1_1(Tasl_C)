#include <iostream>
#include <string>

using namespace std;

typedef struct Item {
    Item* next = nullptr;
    char data;
} Item;

typedef Item* List;

int main() {
    string line;
    getline(cin, line);
    int len = line.length();

    List stack = nullptr;

    for (int i = 0; i < len; i++) {
        if (line[i] == '(' or line[i] == '[' or line[i] == '{') {
            List newItem = new Item;
            newItem->data = line[i];
            newItem->next = stack;
            stack = newItem;
        }
        else if (line[i] == ')' or line[i] == ']' or line[i] == '}') {
            char openBracket = (line[i] == ')' ? '(' : (line[i] == ']' ? '[' : '{'));
            if (stack == nullptr) {
                cout << "no";
                return 0;
            }
            List topItem = stack;
            char value = topItem->data;
            stack = stack->next;
            delete topItem;
            char popped = value;
            if (popped != openBracket) {
                cout << "no";
                return 0;
            }
        }
    }

    if (stack == nullptr) {
        cout << "yes";
    }
    else {
        cout << "no";
    }

    return 0;
}
