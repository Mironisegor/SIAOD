#include <iostream>
using namespace std;

struct Item {
	Item* next = nullptr;
	int data;
};

typedef Item* List;

int main() {
	int n, k, arr[10000], count = 0, counter = 1;
	cin >> n >> k;
	List tail = nullptr;

	for (int i = 0; i < n; i++) {
		if (!tail) {
			tail = new Item;
			tail->data = counter;
			tail->next = tail;
			counter++;
		}
		else {
			List ptr = tail->next;
			List p = new Item;
			p->data = counter;
			tail->next = p;
			p->next = ptr;
			tail = p;
			counter++;
		}
	}

	while (tail->next != tail) {
		for (int i = 0; i < k - 1; i++) {
			tail = tail->next;
		}
		List ptr = tail->next->next;
		arr[count] = tail->next->data;
		count++;
		delete tail->next;
		tail->next = ptr;
	}
	arr[count] = tail->data;
	count++;
	delete tail;

	for (int i = 0; i < count; i++) {
		cout << arr[i] << ' ';
	}

	return 0;
}
