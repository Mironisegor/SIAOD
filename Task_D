//Суперсумма

#include <iostream>
#include <vector>
#include <algorithm>
#include <utility>
using namespace std;

int main() {
	int n;
	cin >> n;
	vector <pair <int, int>> nums(n);

	for (int i = 0; i < n; i++) {
		int t;
		cin >> t;
		nums[i].first = t;
		nums[i].second = i;
	}
	vector <int> ans(n);
	sort(nums.begin(), nums.end());

	for (int i = 0; i < n; i++) {
		for (int j = i+1; j <n; j++) {
			int num = nums[i].first + nums[j].first;
			if (num <= nums[n - 1].first) {
				auto temp = *lower_bound(nums.begin(), nums.end(), make_pair(num, -1));
				if (num == temp.first) {
					ans[temp.second]++;
					int index = lower_bound(nums.begin(), nums.end(), make_pair(num, -1)) - nums.begin();
					while (index != (n - 1) and nums[index + 1].first == num) {
						ans[nums[index + 1].second]++;
						index++;
					}
				}
			}
		}
	}

	for (int i = 0; i < n; i++) {
		cout << ans[i] << endl;
	}
	return 0;
}
