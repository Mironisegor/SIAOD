//Игра в пьяницу
#include<iostream>

using namespace std;

struct Item {
	Item* next = nullptr;
	int data = 0;
};

typedef Item* List;

int main() {
	unsigned long long res = 0;
	List Head1 = nullptr;
	List Head2 = nullptr;
	List p1 = nullptr;
	List p2 = nullptr;
	List p = nullptr;

	for (int i = 0; i < 5; i++) {
		if (!Head1) {
			Head1 = new Item;
			p1 = Head1;
		}
		else {
			p1->next = new Item;
			p1 = p1->next;
		}
		cin >> p1->data;
	}
	p1->next = nullptr;
	List Tail1 = p1;
	p1 = Head1;

	for (int i = 0; i < 5; i++) {
		if (!Head2) {
			Head2 = new Item;
			p2 = Head2;
		}
		else {
			p2->next = new Item;
			p2 = p2->next;
		}
		cin >> p2->data;
	}
	p2->next = nullptr;
	List Tail2 = p2;
	p2 = Head2;

	while (p1 != nullptr && p2 != nullptr && (res < 1000000)) {
		res++;

		if (p1->data == 0 && p2->data == 9) {
			p = new Item;
			p->data = p1->data;
			Tail1->next = p;
			p->next = new Item;
			p->next->data = p2->data;
			Tail1 = p->next;
		}
		else if (p1->data == 9 && p2->data == 0) {
			p = new Item;
			p->data = p1->data;
			Tail2->next = p;
			p->next = new Item;
			p->next->data = p2->data;
			Tail2 = p->next;
		}
		else if (p1->data > p2->data) {
			p = new Item;
			p->data = p1->data;
			Tail1->next = p;
			p->next = new Item;
			p->next->data = p2->data;
			Tail1 = p->next;
		}
		else {
			p = new Item;
			p->data = p1->data;
			Tail2->next = p;
			p->next = new Item;
			p->next->data = p2->data;
			Tail2 = p->next;
		}

		p = p1;
		p1 = p1->next;
		delete p;
		p = p2;
		p2 = p2->next;
		delete p;
	}

	if (res >= 1000000) {
		cout << "botva";
	}
	else if (p1 == nullptr) {
		cout << "second " << res;
	}
	else {
		cout << "first " << res;
	}

	return 0;
}
